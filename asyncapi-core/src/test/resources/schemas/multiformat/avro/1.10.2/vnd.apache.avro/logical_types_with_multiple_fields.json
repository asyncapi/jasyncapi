{
  "schemaFormat": "application/vnd.apache.avro;version=1.10.2",
  "schema": {
    "namespace": "schema.common",
    "type": "record",
    "name": "Action",
    "fields": [
      {
        "name": "name",
        "type": "string"
      },
      {
        "name": "uuid",
        "type": "string"
      },
      {
        "name": "time",
        "type": {
          "type": "long",
          "logicalType": "timestamp-millis"
        }
      },
      {
        "name": "requestId",
        "type": [
          "null",
          "string"
        ],
        "default": null
      }
    ]
  }
}