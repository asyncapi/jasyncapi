{
    "schemaFormat": null,
    "schema": {
        "$id": "https://example.com/complex-object.schema.json",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Complex Object",
        "type": "object",
        "properties": {
            "name": {
                "type": "string"
            },
            "age": {
                "type": "integer",
                "minimum": 0
            },
            "address": {
                "type": "object",
                "properties": {
                    "street": {
                        "type": "string"
                    },
                    "city": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "postalCode": {
                        "type": "string",
                        "pattern": "\\d{5}"
                    }
                },
                "required": ["street", "city", "state", "postalCode"]
            },
            "hobbies": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "required": ["name", "age"]
    }
}
