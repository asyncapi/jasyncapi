{
  "bindings": {
    "kafka": {
      "key": {
        "type": "string"
      },
      "bindingVersion": "0.4.0"
    }
  },
  "payload": {
    "type": "array",
    "items": {
      "type": "object",
      "required": [
        "id",
        "name",
        "done"
      ],
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "done": {
          "type": "boolean"
        }
      }
    }
  }
}